{
    "name": "symfonyboot/symfonyboot-bundle",
    "description": "This package helps you easily transform your request payload into objects directly in the controller via annotations.",
    "keywords": ["request","payload","controllers","annotations"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Silas Yudi",
            "email": "symfonyboot@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/annotations": "^1.10",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/orm": "^2",
        "symfony/flex": "^1.3",
        "symfony/framework-bundle": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SymfonyBoot\\SymfonyBootBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
